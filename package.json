{
  "name": "nfl-rushing",
  "version": "1.0.0",
  "scripts": {
    "lint": "npx eslint --max-warnings 0 src",
    "test": "jest",
    "test:watch": "jest --watch",
    "prettify": " pretty-quick --staged",
    "precommit": "run-s lint test prettify",
    "start": "npm run prebuild-server && run-p prebuild-server dev:*",
    "prebuild-server": "webpack --config webpack.server.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:run-server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "build": "webpack --config webpack.client.prod.js"
  },
  "author": "Adil Nurtayev",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  }
}
